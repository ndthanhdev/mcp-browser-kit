{
	"extends": "../../tsconfig.options.json",
	"include": [
		"**/*",
		"**/.server/**/*",
		"**/.client/**/*",
		".react-router/types/**/*"
	],
	"compilerOptions": {
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ES2022"
		],
		"types": [
			"node",
			"vite/client"
		],
		"moduleResolution": "bundler",
		"jsx": "react-jsx",
		"rootDirs": [
			".",
			"./.react-router/types"
		],
		"baseUrl": ".",
		"paths": {
			"~/*": [
				"./app/*"
			]
		},
		"noEmit": true,
		"resolveJsonModule": true,
		"outDir": "../../.moon/cache/types/apps/ext-e2e-test-app"
	}
}
