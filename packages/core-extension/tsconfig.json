{
	"extends": "../../tsconfig.options.json",
	"include": [
		"**/*"
	],
	"references": [
		{
			"path": "../types"
		},
		{
			"path": "../utils"
		}
	],
	"compilerOptions": {
		"outDir": "../../.moon/cache/types/packages/core-extension",
		"paths": {
			"@mcp-browser-kit/utils/*": [
				"../utils/src/*"
			],
			"@mcp-browser-kit/utils": [
				"../utils/src/index.ts"
			],
			"@mcp-browser-kit/types": [
				"../types/src/index.ts"
			],
			"@mcp-browser-kit/types/*": [
				"../types/src/*"
			]
		}
	}
}
