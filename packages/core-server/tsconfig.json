{
	"extends": "../../tsconfig.options.json",
	"include": [
		"**/*"
	],
	"references": [
		{
			"path": "../core-extension"
		}
	],
	"compilerOptions": {
		"outDir": "../../.moon/cache/types/packages/core-server",
		"paths": {
			"@mcp-browser-kit/browser-extension": [
				"../core-extension/src/index.ts"
			],
			"@mcp-browser-kit/browser-extension/*": [
				"../core-extension/src/*"
			],
			"@mcp-browser-kit/core-extension": [
				"../core-extension/src/index.ts"
			],
			"@mcp-browser-kit/core-extension/*": [
				"../core-extension/src/*"
			]
		}
	}
}
