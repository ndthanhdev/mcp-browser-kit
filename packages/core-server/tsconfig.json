{
	"extends": "../../tsconfig.options.json",
	"include": [
		"**/*"
	],
	"references": [
		{
			"path": "../core-extension"
		},
		{
			"path": "../types"
		},
		{
			"path": "../utils"
		}
	],
	"compilerOptions": {
		"outDir": "../../.moon/cache/types/packages/core-server",
		"paths": {
			"@mcp-browser-kit/core-extension": [
				"../core-extension/src/index.ts"
			],
			"@mcp-browser-kit/core-extension/*": [
				"../core-extension/src/*"
			],
			"@mcp-browser-kit/types": [
				"../types/src/index.ts"
			],
			"@mcp-browser-kit/types/*": [
				"../types/src/*"
			],
			"@mcp-browser-kit/utils": [
				"../utils/src/index.ts"
			],
			"@mcp-browser-kit/utils/*": [
				"../utils/src/*"
			]
		}
	}
}
