{
	"extends": "../../tsconfig.options.json",
	"include": [
		"**/*"
	],
	"compilerOptions": {
		"outDir": "../../.moon/cache/types/packages/server-driving-trpc-controller",
		"paths": {
			"@mcp-browser-kit/core-server": [
				"../core-server/src/index.ts"
			],
