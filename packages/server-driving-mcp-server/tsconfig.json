{
	"extends": "../../tsconfig.options.json",
	"include": [
		"**/*"
	],
	"compilerOptions": {
		"outDir": "../../.moon/cache/types/packages/server-driving-mcp-server",
		"paths": {
			"@mcp-browser-kit/core-server": [
				"../core-server/src/index.ts"
			],
			"@mcp-browser-kit/core-server/*": [
				"../core-server/src/*"
			],
			"@mcp-browser-kit/types": [
				"../types/src/index.ts"
			],
			"@mcp-browser-kit/types/*": [
				"../types/src/*"
			]
		}
	},
	"references": [
		{
			"path": "../core-server"
		},
		{
			"path": "../types"
		}
	]
}
